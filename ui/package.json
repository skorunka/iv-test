{
	"name": "guests-manager",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"ng": "ng",
		"build": "yarn run env && ng build --prod",
		"start": "yarn run env && ng serve --aot --proxy-config proxy.conf.js",
		"start:hmr": "yarn run env && ng serve --configuration hmr --aot --proxy-config proxy.conf.js",
		"start:dev": "run-p json-server start:hmr",
		"lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
		"test": "yarn run env && ng test",
		"test:ci": "yarn run env && yarn run lint && ng test --code-coverage --watch=false",
		"e2e": "yarn run env && cypress open",
		"translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean -sort --marker extract",
		"env": "ngx-scripts env npm_package_version",
		"generate": "ng generate",
		"stylelint-check": "stylelint-config-prettier-check",
		"precommit": "pretty-quick --staged && yarn lint",
		"json-server": "json-server --watch _mocks/api/db.json --routes _mocks/api/routes.json"
	},
	"dependencies": {
		"@angular/animations": "6.1.9",
		"@angular/common": "6.1.9",
		"@angular/compiler": "6.1.9",
		"@angular/core": "6.1.9",
		"@angular/forms": "6.1.9",
		"@angular/http": "6.1.9",
		"@angular/platform-browser": "6.1.9",
		"@angular/platform-browser-dynamic": "6.1.9",
		"@angular/router": "6.1.9",
		"@fortawesome/fontawesome-free": "5.3.1",
		"@ng-bootstrap/ng-bootstrap": "3.2.2",
		"@ngrx/effects": "6.1.0",
		"@ngrx/entity": "6.1.0",
		"@ngrx/store": "6.1.0",
		"@ngrx/store-devtools": "6.1.0",
		"@ngx-translate/core": "10.0.2",
		"bootstrap": "4.1.3",
		"core-js": "2.5.7",
		"lodash": "4.17.11",
		"ngx-toastr": "9.1.0",
		"rxjs": "6.3.3",
		"zone.js": "0.8.26"
	},
	"devDependencies": {
		"@angular-builders/jest": "1.2.4",
		"@angular-devkit/build-angular": "0.8.4",
		"@angular/cli": "6.2.4",
		"@angular/compiler-cli": "6.1.9",
		"@angular/language-service": "6.1.9",
		"@angularclass/hmr": "2.1.3",
		"@biesbjerg/ngx-translate-extract": "2.3.4",
		"@ngrx/schematics": "6.1.0",
		"@ngx-rocket/scripts": "3.0.1",
		"@types/lodash": "4.14.116",
		"@types/node": "10.11.4",
		"codelyzer": "4.4.4",
		"cypress": "3.1.0",
		"htmlhint": "0.10.1",
		"https-proxy-agent": "2.2.1",
		"husky": "1.1.0",
		"jest": "23.6.0",
		"ngrx-store-freeze": "0.2.4",
		"npm-run-all": "4.1.3",
		"prettier": "1.14.3",
		"pretty-quick": "1.7.0",
		"stylelint": "9.6.0",
		"stylelint-config-prettier": "4.0.0",
		"stylelint-scss": "3.3.1",
		"ts-node": "7.0.1",
		"tslint": "5.11.0",
		"typescript": "2.9.2"
	}
}
